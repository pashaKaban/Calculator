<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SimpleCalculate">
    <TextView
            android:layout_width="368dp"
            android:layout_height="73dp"
            android:id="@+id/textView2"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.503"
            android:layout_marginStart="8dp"/>

    <Button
            android:text="1"
            android:layout_width="69dp"
            android:layout_height="45dp"
            android:id="@+id/button2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginTop="160dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:onClick="onClickOne"/>
    <Button
            android:text="2"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button3"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:onClick="onClickTwo"/>
    <Button
            android:text="3"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button4"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:onClick="onClickThree"/>
    <Button
            android:text="4"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button5"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:onClick="onClickFour"/>
    <Button
            android:text="5"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button6"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button5"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:onClick="onClickFive"/>
    <Button
            android:text="6"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button8"
            android:onClick="onClickSix"
            android:layout_marginTop="160dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintStart_toEndOf="@+id/button2"
            android:layout_marginStart="8dp"/>
    <Button
            android:text="7"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button7"
            android:onClick="onClickSeven"
            app:layout_constraintStart_toEndOf="@+id/button3"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button8"/>
    <Button
            android:text="8"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button9"
            app:layout_constraintStart_toEndOf="@+id/button4"
            android:layout_marginStart="8dp"
            android:onClick="onClickEight"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button7"/>
    <Button
            android:text="9"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button10"
            app:layout_constraintStart_toEndOf="@+id/button5"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button9"
            android:onClick="onClickNine"/>
    <Button
            android:text="0"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button12"
            app:layout_constraintStart_toEndOf="@+id/button6"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button10"
            android:onClick="onClickZero"/>
    <Button
            android:text="%"
            android:layout_width="76dp"
            android:layout_height="45dp"
            android:id="@+id/button13"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:onClick="onClickPercent"
            app:layout_constraintStart_toEndOf="@+id/button8"
            android:layout_marginStart="8dp"
            android:layout_marginTop="160dp"/>
    <Button
            android:text="sqrt"
            android:layout_width="76dp"
            android:layout_height="45dp"
            android:id="@+id/button14"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button13"
            android:onClick="onClickSqrt"
            app:layout_constraintStart_toEndOf="@+id/button7"
            android:layout_marginStart="8dp"/>
    <Button
            android:text="1/x"
            android:layout_width="76dp"
            android:layout_height="45dp"
            android:id="@+id/button15"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button14"
            app:layout_constraintStart_toEndOf="@+id/button9"
            android:layout_marginStart="8dp"
            android:onClick="onClickReverce"/>
    <Button
            android:text="C"
            android:layout_width="76dp"
            android:layout_height="45dp"
            android:id="@+id/button16"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button15"
            app:layout_constraintStart_toEndOf="@+id/button10"
            android:layout_marginStart="8dp"
            android:onClick="onClickFullDelete"/>
    <Button
            android:text="Delete"
            android:layout_width="78dp"
            android:layout_height="45dp"
            android:id="@+id/button17"
            app:layout_constraintStart_toEndOf="@+id/button12"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button16"
            android:onClick="onClickDelete"
            app:layout_constraintEnd_toStartOf="@+id/button22"
            android:layout_marginEnd="16dp"/>
    <Button
            android:text="/"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button18"
            app:layout_constraintStart_toEndOf="@+id/button13"
            android:layout_marginStart="8dp"
            android:layout_marginTop="160dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:onClick="onClickDivision"/>

    <Button
            android:text="*"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button19"
            app:layout_constraintStart_toEndOf="@+id/button14"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button18"
            android:onClick="onClickMultiplication"/>
    <Button
            android:text="-"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button20"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button19"
            app:layout_constraintStart_toEndOf="@+id/button15"
            android:layout_marginStart="8dp"
            android:onClick="onClickMinus"
    />
    <Button
            android:text="+"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button21"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button20"
            app:layout_constraintStart_toEndOf="@+id/button16"
            android:layout_marginStart="8dp"
            android:onClick="onClickPlus"/>
    <Button
            android:text="="
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:id="@+id/button22"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button21"
            app:layout_constraintStart_toEndOf="@+id/button17"
            android:layout_marginStart="8dp"
            android:onClick="onClickEqually"/>
    <Button
            android:text="."
            android:layout_width="60dp"
            android:layout_height="45dp"
            android:id="@+id/button23"
            app:layout_constraintStart_toEndOf="@+id/button18"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="160dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:onClick="onClickPoint"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text=""
            android:layout_width="366dp"
            android:layout_height="54dp"
            android:id="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintHorizontal_bias="0.0"/>
</android.support.constraint.ConstraintLayout>